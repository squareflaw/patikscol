extends ../includes/blogPostTemplate
append config
  -var post = posts[posts.length - 2];  
  -title = post.blogPostTitle;
  -headTitle = post.blogPostTitle + " | " + site.name;
  -description = post.description;
  -ogUrl= site.domain + post.Url;
  -ogImgUrl = site.domain + post.imgUrl;

block blogPostUrl    
  include:markdown-it(html) ../../blogPosts/Los-gatos-se-banan.md
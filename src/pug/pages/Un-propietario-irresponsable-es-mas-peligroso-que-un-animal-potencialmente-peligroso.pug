extends ../includes/blogPostTemplate
append config
  -var post = posts[posts.length - 11];    
  -title = post.blogPostTitle;
  -headTitle = post.blogPostTitle + " | " + site.name;
  -description = post.description;
  -ogUrl= site.domain + post.Url;
  -ogImgUrl = site.domain + post.imgUrl;

block blogPostUrl
  include:markdown-it(html) ../../blogPosts/Un-propietario-irresponsable-es-mas-peligroso-que-un-animal-potencialmente-peligroso.md